danger(A) :-
   harmonizing(A,uranus,pluto).
danger(A) :-
   ascendant(A,libra).
danger(karl).
popular(A) :-
   harmonizing(A,moon,jupiter).
popular(A) :-
   discordant(A,uranus,neptune).
popular(roosevelt).
popular(king).
intelligent(A).
pacifist(A) :-
   blending(A,jupiter,pluto).
pacifist(einstein).
